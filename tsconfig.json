{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "ESNext",
    "jsx": "react",
    "skipLibCheck": true,
    "outDir": "_typescript"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  
}