{
  "name": "cinema-viewer",
  "version": "0.0.1",
  "description": "Advanced cinema viewer",
  "main": "index.js",
  "sideEffects": [
    "*.styl",
    "*.css"
  ],
  "engines": {
    "node": "10.15.2"
  },
  "scripts": {
    "start": "node build/server.js",
    "dev": "webpack-dev-server --mode development --progress --colors",
    "serv": "cross-env NODE_ENV=development nodemon --exec babel-node -r node_modules/dotenv/config src/server.ts --extensions \".ts,.tsx,.js,.jsx\"",
    "debug": "cross-env NODE_ENV=development babel-node -r node_modules/dotenv/config src/server.ts --extensions \".ts,.tsx,.js,.jsx\" --inspect",
    "clean": "rimraf build/ dist/ lib/",
    "lint": "standard --parser babel-eslint --fix \"*.ts,*.tsx\"",
    "test": "yarn jest",
    "test:watch": "yarn test --watch",
    "cypress:open": "yarn cypress open",
    "check:src": "yarn lint && yarn test",
    "check:types": "yarn tsc --noEmit && rimraf _typescript/",
    "check": "yarn check:src && yarn check:types",
    "commit": "yarn git-cz",
    "build:bundle": "webpack --mode production --progress --colors",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir build --extensions \".ts,.tsx,.js,.jsx\"",
    "build": "yarn clean && yarn run check && yarn build:server && yarn build:bundle",
    "heroku-postbuild": "yarn build"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loadable/component": "^5.6.1",
    "@loadable/server": "^5.6.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "connect-gzip-static": "^2.1.1",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongodb": "^3.1.13",
    "open-iconic": "^1.1.1",
    "p-min-delay": "^2.0.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-youtube": "^7.9.0",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "shortid": "^2.2.14",
    "styled-components": "^4.1.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "7.0.0",
    "@loadable/babel-plugin": "^5.6.0",
    "@loadable/webpack-plugin": "^5.5.0",
    "@types/connect-mongo": "^0.0.40",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/loadable__component": "^5.6.0",
    "@types/loadable__server": "^5.2.1",
    "@types/mongodb": "^3.1.19",
    "@types/node-schedule": "^1.2.3",
    "@types/p-min-delay": "^2.0.0",
    "@types/passport": "^1.0.0",
    "@types/passport-oauth2": "^1.4.8",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-youtube": "^7.6.1",
    "@types/reactstrap": "^7.1.3",
    "@types/spdy": "^3.4.4",
    "@types/styled-components": "^4.1.10",
    "@types/webpack-env": "^1.13.7",
    "@types/xml2js": "^0.4.3",
    "autoprefixer": "^9.4.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "commitizen": "^4.0.3",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^23.6.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "jest-styled-components": "^6.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.9",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rupture": "^0.7.1",
    "standard": "^12.0.1",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.0",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.1.14"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "standard": {
    "globals": [
      "describe",
      "test",
      "expect",
      "jest",
      "beforeEach"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "src/components/*",
      "src/store/*",
      "src/assets/*",
      "src/client.ts"
    ],
    "ext": "js,jsx,ts,tsx"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
