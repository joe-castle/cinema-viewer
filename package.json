{
  "name": "cinema-viewer",
  "version": "0.0.0-development",
  "description": "Advanced cinema viewer",
  "main": "index.js",
  "sideEffects": [
    "*.styl",
    "*.css"
  ],
  "engines": {
    "node": "10.15.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joesmith100/cinema-viewer.git"
  },
  "scripts": {
    "start": "node build/server.js",
    "dev": "webpack-dev-server --mode development --progress --colors",
    "serv": "cross-env NODE_ENV=development nodemon --exec babel-node -r node_modules/dotenv/config src/server.ts --extensions \".ts,.tsx,.js,.jsx\"",
    "debug": "cross-env NODE_ENV=development babel-node -r node_modules/dotenv/config src/server.ts --extensions \".ts,.tsx,.js,.jsx\" --inspect",
    "postinstall": "node build-script/setup.js",
    "commit": "yarn build && yarn clean && yarn git-cz && yarn push",
    "clean": "rimraf build/ dist/ lib/",
    "lint": "standard --parser babel-eslint --fix \"*.ts,*.tsx\"",
    "test": "yarn jest",
    "test:watch": "yarn test --watch",
    "cypress:open": "yarn cypress open",
    "check:src": "yarn lint && yarn test",
    "check:types": "yarn tsc --noEmit && rimraf _typescript/",
    "check": "yarn check:src && yarn check:types",
    "build:bundle": "webpack --mode production --progress --colors",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir build --extensions \".ts,.tsx,.js,.jsx\"",
    "build": "yarn clean && yarn run check && yarn build:server && yarn build:bundle",
    "release": "semantic-release",
    "heroku-postbuild": "yarn build"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@loadable/component": "^5.10.2",
    "@loadable/server": "^5.10.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "connect-gzip-static": "^2.1.1",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "mongodb": "^3.2.7",
    "open-iconic": "^1.1.1",
    "p-min-delay": "^3.1.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-youtube": "^7.9.0",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2",
    "shortid": "^2.2.14",
    "styled-components": "^4.3.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.5.5",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@types/connect-mongo": "^0.0.43",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.9.1",
    "@types/mongodb": "^3.1.30",
    "@types/node-schedule": "^1.2.3",
    "@types/p-min-delay": "^3.0.0",
    "@types/passport": "^1.0.0",
    "@types/passport-oauth2": "^1.4.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-youtube": "^7.6.2",
    "@types/reactstrap": "^8.0.1",
    "@types/spdy": "^3.4.4",
    "@types/styled-components": "^4.1.18",
    "@types/webpack-env": "^1.14.0",
    "@types/xml2js": "^0.4.4",
    "autoprefixer": "^9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "commitizen": "^4.0.3",
    "compression-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "file-loader": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "jest-styled-components": "^6.3.3",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.1",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rupture": "^0.7.1",
    "semantic-release": "^15.13.18",
    "standard": "^13.1.0",
    "standard-loader": "^7.0.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.7.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "standard": {
    "globals": [
      "describe",
      "test",
      "expect",
      "jest",
      "beforeEach"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": ["./jest.setup.js"],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "src/components/*",
      "src/store/*",
      "src/assets/*",
      "src/client.ts"
    ],
    "ext": "js,jsx,ts,tsx"
  },
  "config": {
    "commitizen": {
      "path": "../cz-conventional-changelog/index.js"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer", 
      "@semantic-release/release-notes-generator", 
      "@semantic-release/github"]
  }
}
